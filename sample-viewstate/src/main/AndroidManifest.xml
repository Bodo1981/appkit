<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.christianbahl.appkit.sampleviewstate">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity
        android:name=".activity_toolbar.ActivityToolbar"/>

    <activity
        android:name=".activity_toolbar_fragment.ActivityToolbarFragment"/>

    <activity
        android:name=".activity_toolbar_tabs.ActivityToolbarTabs"/>

    <activity
        android:name=".fragment_recyclerview.FragmentActivtyRecyclerView"/>

    <activity
        android:name=".fragment_recyclerview_ptr.FragmentActivtyRecyclerViewPtr"/>
  </application>

</manifest>
